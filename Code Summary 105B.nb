      FractionBox[
       RowBox[{"\[Chi]", " ", 
        StyleBox[
         RowBox[{"\[CapitalDelta]", 
          StyleBox["t",
           FontSlant->"Italic"]}]]}], 
       SuperscriptBox[
        StyleBox[
         RowBox[{"\[CapitalDelta]", 
          StyleBox["x",
           FontSlant->"Italic"]}]], "2"]], 
      RowBox[{"(", 
