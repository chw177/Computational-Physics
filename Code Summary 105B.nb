Cell[CellGroupData[{

Cell["Originally ", "Subsection",
 CellChangeTimes->{{3.6991125089638557`*^9, 3.69911251050097*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubsuperscriptBox["T", "j", "n"], "-", 
     SubsuperscriptBox["T", "j", 
      RowBox[{"n", "-", "1"}]]}], "=", 
    RowBox[{
     RowBox[{
      StyleBox[
       RowBox[{"\[CapitalDelta]", 
